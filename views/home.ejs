<style>
    html, body {
        height: 100%;
    }

    .full-height {
        height: calc(100% - 64px);
        margin: 0;
    }

    .empty {
        padding: 0;
    }

    .image123{
        clear: both;
        margin-top: 10em;
    }

    .title-box {
        margin-top: 5em;
        clear:both;
    }

    .title-text {
        color: white;
        margin-top: 0;
        font-weight: 700;
        text-shadow: 0 0 0.5em #FFFFFF;
    }

    .subtitle-text {
        font-size: 2em;
        color:white;
    }

    .small-glyph {
        margin-left: 5em;
