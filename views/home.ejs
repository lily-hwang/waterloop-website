<style>
    html, body {
        height: 100%;
    }
    @media only screen and (min-width: 993px) {
      .full-height {
          height: calc(100% - 64px);
          margin: 0;
      }
    }
    @media only screen and (max-width: 992px) {
      .full-height {
          height: calc(100% - 56px);
          margin: 0;
      }
    }

    .empty {
        padding: 0;
    }

    .image123{
        clear: both;
        /*margin-top: 10em;*/
    }

    .title-box {
        margin-top: 5em;
        clear:both;
    }

    .title-text {
        color: white;
        margin-top: 0;
        font-weight: 700;
        text-shadow: 0 0 0.5em #FFFFFF;
    }

    .subtitle-text {
        font-size: 2.3vmax;
        color:white;
    }

    .small-glyph {
        margin-left: 5em;
    }
</style>

<div class="parallax-container full-height" style="padding: 70px 0;">
